<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.bangkit.manduin.ui.HomeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_explore"
        android:name="com.bangkit.manduin.ui.ExploreFragment"
        tools:layout="@layout/fragment_explore" />

    <fragment
        android:id="@+id/navigation_bookmark"
        android:name="com.bangkit.manduin.ui.BookmarkFragment"
        tools:layout="@layout/fragment_bookmark" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.bangkit.manduin.ui.ProfileFragment"
        tools:layout="@layout/fragment_profile"/>
</navigation>